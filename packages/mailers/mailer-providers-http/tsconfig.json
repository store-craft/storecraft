{
  "compileOnSave": false,
  "compilerOptions": {
    "noEmit": true,
    "allowJs": true,
    "maxNodeModuleJsDepth": 10,
    "checkJs": true,
    "target": "ESNext",
    "resolveJsonModule": true,
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "composite": true,
  },
  "include": ["*", "*/*"]
}